						"topmiddle": {"sx": 37, "sy": 2, "width": 20, "height": 11, "repeatv": false, "repeath": true, "offset": [0,21,0,0], "stackw": "topleft"},
						"topright": {"sx": 71, "sy": 2, "width": 21, "height": 21,  "repeatv": false, "repeath": false, "offset": [0,0,0,0], "stackw": "topmiddle"},
